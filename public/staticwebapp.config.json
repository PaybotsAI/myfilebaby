{
  "routes": [
    {
      "route": "/",
      "allowedRoles": ["FileBabyBasicUsers"]
    }
  ],
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html"
    }
  },
  "globalHeaders": {
    "content-security-policy": "default-src 'self'; img-src https://*; child-src 'none';"
  },
  "mimeTypes": {
    ".json": "text/json"
  }
}
